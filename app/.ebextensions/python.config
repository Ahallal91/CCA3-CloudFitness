container_commands:
    command1:
        command: cd /home/ec2-user && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt && export AWS_DEFAULT_REGION=ap-southeast-2 && python3 create_table.py && deactivate

option_settings:
    "aws:elasticbeanstalk:container:python":
        WSGIPath: application:application